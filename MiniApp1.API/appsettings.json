{ //MiniApi1 serverimdaki apilere istek atabilmesi için tokenimin aşağıdaki bilgilerle oluşturulması gerekiyor.
  "TokenOption": {
    "Audience": [ "www.miniapi1.com" ],//bu apinin serverini izin verilen audience olarak ekliyoruz.
    "Issuer": "www.authserver.com",//dağıtıcı serverin authserver.com olduğunu belirtiyoruz.
    "AccessTokenExpiration": 5,
    "SecurityKey": "mysecuritykeymysecuritykeymysecuritykeymysecuritykey"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
